{
  "name": "solvidi-website",
  "version": "4.3.0",
  "private": false,
  "license": "UNLICENSED",
  "author": "Samuel Kedziora <samuel@front-up.pro>",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint --ext .jsx,.tsx .",
    "start": "react-scripts start",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!gsap)"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "classnames": "^2.3.2",
    "gsap": "^3.10.4",
    "i18next": "^21.8.12",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "react": "^18.2.0",
    "react-audio-player": "^0.17.0",
    "react-country-flag": "^3.0.2",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.2",
    "react-i18next": "^11.17.4",
    "react-scripts": "^5.0.1",
    "react-scroll": "^1.8.7",
    "react-transition-group": "^4.4.2",
    "react-tsparticles": "^2.1.3",
    "react-typing-effect": "^2.0.5",
    "tsparticles": "^2.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.6",
    "@types/react-scroll": "^1.8.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/react-typing-effect": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "nock": "^13.2.8",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "sass": "^1.62.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4",
    "yarn-upgrade-all": "^0.7.1"
  }
}
